   "source": [
    "# Determine how many years it took for companies to reach unicorn status\n",
    "\n",
    "companies[\"Years To Join\"] = companies[\"Date Joined\"].dt.year - companies[\"Founded Year\"]\n",
    "companies.head()\n"
   ]
